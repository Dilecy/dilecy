  "scripts": {
    "start": "cross-env BUILD_ENV=development electron-forge start",
    "webdev": "cross-env BUILD_ENV=development webpack-dev-server --open --compress --hot --config config/webpack.webdev.config.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "pree2etest": "cross-env BUILD_ENV=e2etest node ./tools/build.js",
