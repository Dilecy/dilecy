    "rebuild": "electron-rebuild -f -w sqlite3",
    "ciphertest": "electron test/ciphertest.js",
    "prenativetest": "tsc -p test/native/tsconfig.json",
    "nativetest": "electron test-out/test/native/native-test.js",
